package ex_aula29;

import java.util.Scanner;

public class Ex_1 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int x, y, resultado;

		x = sc.nextInt();

		y = sc.nextInt();
		
		resultado = x + y;

		System.out.printf("Os números digitados foram %d e %d", x, y);
		System.out.printf("A soma dos números digitados é %d", resultado);

		sc.close();

	}

}
